<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kemizhibo.kemizhibo.yhr.activity.personcenters.PersonCenterShouCangActivity">
    <include layout="@layout/pubic_title_bar"/>
    <include layout="@layout/line"/>
    <com.liaoinstan.springview.widget.SpringView
        android:layout_marginTop="@dimen/res_17.5dp"
        android:layout_below="@+id/search_view_box"
        android:id="@+id/collection_box_springview"
        android:layout_marginLeft="@dimen/res_10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <android.support.v7.widget.RecyclerView
            android:id="@+id/collection_box_recyclerview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </android.support.v7.widget.RecyclerView>
    </com.liaoinstan.springview.widget.SpringView>
</LinearLayout>
